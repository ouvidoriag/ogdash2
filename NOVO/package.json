{
  "name": "ouvidoria-dashboard",
  "version": "3.0.0",
  "description": "Dashboard estratégico da Ouvidoria Geral de Duque de Caxias - Versão Otimizada e Refatorada",
  "private": true,
  "type": "module",
  "main": "src/server.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "postinstall": "node scripts/setup/setup.js",
    "prestart": "node scripts/setup/setup.js",
    "start": "node src/server.js",
    "dev": "node src/server.js",
    "build": "echo 'No build step required - aplicação Node.js pura'",
    "setup": "node scripts/setup/setup.js",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate deploy",
    "prisma:push": "npx prisma db push",
    "prisma:studio": "npx prisma studio",
    "update:excel": "node scripts/data/updateFromExcel.js",
    "update:sheets": "node scripts/data/updateFromGoogleSheets.js",
    "test:sheets": "node scripts/test/testGoogleSheets.js",
    "pipeline": "node scripts/data/runPipeline.js",
    "setup:python": "node scripts/setup/setup-python.js",
    "fix:prisma": "node scripts/maintenance/fix-prisma.js",
    "map:system": "node scripts/maintenance/mapear-sistema.js",
    "import:zeladoria": "node scripts/data/importZeladoria.js",
    "test:pages": "node scripts/test/run-page-tests.js",
    "test:all": "node scripts/test/test-all-pages.js",
    "gmail:auth": "node scripts/email/autorizar-gmail.js",
    "email:saude": "node scripts/email/enviar-email-saude.js",
    "email:real": "node scripts/email/enviar-email-real.js",
    "create:users": "node scripts/setup/createUsers.js",
    "docs:generate": "node scripts/generate-api-docs.js",
    "docs:json": "node scripts/generate-api-docs.js --format json",
    "docs:swagger": "node scripts/generate-api-docs.js --format swagger",
    "analyze:architecture": "node scripts/analyze-architecture.js"
  },
  "keywords": [
    "ouvidoria",
    "dashboard",
    "duque-de-caxias",
    "prisma",
    "mongodb",
    "gemini-ai"
  ],
  "author": "Ouvidoria Geral de Duque de Caxias",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "express-session": "^1.18.2",
    "googleapis": "^144.0.0",
    "mongodb": "^6.3.0",
    "mongoose": "^9.0.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "prisma": "^5.22.0",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "puppeteer": "^24.31.0"
  }
}
